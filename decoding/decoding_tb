module decoding;

	// Inputs
	reg [1:12] d;

	// Outputs
	wire [4:1] c;
	wire [1:12] temp;

	// Instantiate the Unit Under Test (UUT)
	hamming_decoding uut (
		.d(d), 
		.c(c), 
		.temp(temp)
	);

	initial begin
		// Initialize Inputs
		d = 0;

		// Wait 100 ns for global reset to finish
		#10;
		d = 12'b001110010100;// no error
		#10;
		d = 12'b101110010100;//error at first bit
		#10;
		d = 12'b001100010100;// error at fifth
		
        
		// Add stimulus here

	end
      
endmodule

